{
    "name": "glitchr/well-known",
    "type": "symfony-bundle",
    "description": ".well-known directory integration for Symfony",
    "keywords": ["symfony", "ux", "crisp"],
    "homepage": "https://symfony.com",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyer@glitchr.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Well\\Known\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Well\\Known\\": "./Tests"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^4.0|^5.0|^6.0",
        "symfony/dependency-injection": "^4.0|^5.0|^6.0",
        "symfony/http-kernel": "^4.0|^5.0|^6.0",
        "symfony/filesystem": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "^4.0|^5.0|^6.0",
        "symfony/twig-bundle": "^4.0|^5.0|^6.0",
        "symfony/var-dumper": "^4.0|^5.0|^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
